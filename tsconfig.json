{
  "compilerOptions": {
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": false,
    "module": "es6",
    "target": "es5",
    "allowJs": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "sourceMap": true,
    "strictPropertyInitialization": false,
  }
}
